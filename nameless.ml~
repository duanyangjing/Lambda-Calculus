(* A term is a variable, abstraction or application. *)

(* Ordinary term. Var and Abs include the string representation of the (bound) variable *)
type oterm =
  | oVar of string
  | oAbs of string * oterm
  | oApp of oterm * oterm

type dterm =
  | dVar of int
  | dAbs of dterm
  | dApp of dterm * dterm
